<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape android:shape="rectangle" >
            <corners android:radius="@dimen/uxsdk_2_dp" />

            <stroke
                android:width="1dp"
                android:color="@color/uxsdk_setting_ui_battery_gray" />

            <solid android:color="@android:color/transparent" >
            </solid>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip android:clipOrientation="horizontal" >
            <shape android:shape="rectangle" >
                <stroke
                    android:width="@dimen/uxsdk_6_dp"
                    android:color="#00000000" />

                <corners android:radius="@dimen/uxsdk_1_dp" />

                <solid android:color="@color/uxsdk_setting_ui_battery_green" >
                </solid>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip android:clipOrientation="horizontal" >
            <shape android:shape="rectangle" >
                <stroke
                    android:width="@dimen/uxsdk_6_dp"
                    android:color="#00000000" />

                <corners android:radius="@dimen/uxsdk_1_dp" />

                <solid android:color="@color/uxsdk_setting_ui_battery_green" >
                </solid>
            </shape>
        </clip>
    </item>

</layer-list>