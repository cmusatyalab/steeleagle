<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/uxsdk_dic_color_c8_white" />
            <stroke android:width="@dimen/tab_border_width" android:color="@color/uxsdk_dic_color_c8_white" />
        </shape>
    </item>

    <item android:state_checked="true">
        <shape>
            <solid android:color="@color/uxsdk_dic_color_c8_white" />
            <stroke android:width="@dimen/tab_border_width" android:color="@color/uxsdk_dic_color_c8_white" />
        </shape>
    </item>

    <item>
        <layer-list>
            <item>
                <shape>
                    <solid android:color="@color/uxsdk_white_10_percent" />
                </shape>
            </item>
        </layer-list>
    </item>


</selector>   